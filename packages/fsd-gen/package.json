{
  "name": "@starodubenko/fsd-gen",
  "version": "1.4.1-0",
  "description": "A powerful CLI tool for scaffolding Feature-Sliced Design (FSD) components, slices, and layers.",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./reverse": {
      "types": "./dist/lib/reverse/types.d.ts",
      "import": "./dist/lib/reverse/types.js"
    }
  },
  "bin": {
    "fsd-gen": "dist/cli.js"
  },
  "files": [
    "dist",
    "templates",
    "README.md",
    "package.json"
  ],
  "author": "Rodion Starodubenko",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Starodubenko/fsd-generator.git"
  },
  "keywords": [
    "fsd",
    "feature-sliced-design",
    "generator",
    "cli",
    "react",
    "scaffold"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "tsx src/cli.ts",
    "build": "tsc -p tsconfig.build.json",
    "lint": "eslint .",
    "test": "yarn build && vitest run",
    "test:ui": "yarn build && vitest --ui",
    "test:coverage": "yarn build && vitest run --coverage",
    "prepublishOnly": "yarn build && yarn test"
  },
  "dependencies": {
    "chalk": "^5.4.1",
    "commander": "^13.0.1",
    "enquirer": "^2.4.1",
    "inquirer": "^12.3.0",
    "jiti": "^2.6.1",
    "ts-morph": "^27.0.2",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^25.0.6",
    "@vitest/coverage-v8": "4.0.16",
    "@vitest/ui": "^4.0.16",
    "eslint": "^9.39.1",
    "tsx": "^4.19.2",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite-tsconfig-paths": "^6.0.4",
    "vitest": "^4.0.16"
  }
}
